<RaiseFault async="false" continueOnError="false" enabled="true" name="RaiseFault.InvalidAccessToken">
    <FaultResponse>
        <Set>
            <Payload contentType="text/plain"/>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Access Denied</ReasonPhrase>
        </Set>
        <AssignVariable>
            <Name>ih.error.code</Name>
            <Value>forbidden</Value>
        </AssignVariable>
        <AssignVariable>
            <Name>ih.error.diagnostics</Name>
            <Value>Provided access token is invalid</Value>
        </AssignVariable>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
